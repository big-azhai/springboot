<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liuxiang.springboot.mapper.EmployeeMapper">
    <select id="getEmpById" resultType="com.liuxiang.springboot.bean.Employees">
        select * from t_employee where id=#{id}
    </select>
    <insert id="insertEmp">
        insert into t_employee(username,sex,age,department) values (#{username},#{sex},#{age},#{department})
    </insert>
</mapper>